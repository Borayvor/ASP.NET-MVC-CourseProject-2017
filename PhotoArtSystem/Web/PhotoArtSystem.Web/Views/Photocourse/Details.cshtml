@model Guid

@{
    ViewBag.Title = "Photo course";
}


<div id="photoartsystem-photocourse-details" class="container">
    <h2>@ViewBag.Title</h2>
    <hr />

    @(Html.Action<PhotocourseController>(c => c.GetPhotocourse(Model)))
</div>

@section scripts{
    @Scripts.Render("~/bundles/owlCarousel")
    @Scripts.Render("~/bundles/fancybox")
}

