@using PhotoArtSystem.Web.ViewModels.PhotocourseModels
@model IEnumerable<PhotocourseHomeViewModel>


@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>


<div>
    <h3>Users</h3>
    <br />
    <div class="row">
        <div class="col-md-6">
            Id
        </div>
        <div class="col-md-3">
            Name
        </div>
       
        <div class="col-md-3">
            CreatedOn
        </div>
    </div>
    <hr />
    @foreach (var photocourse in Model)
    {
        <div class="row">
            <div class="col-md-6">
                @photocourse.Id
            </div>
            <div class="col-md-3">
                @photocourse.Name
            </div>            
            <div class="col-md-3">
                @photocourse.CreatedOn.ToLocalTime()
            </div>
        </div>
    }
</div>